{
  "manifest_version": 3,
  "name": "Maps Data Miner",
  "version": "1.0.5",
  "description": "Maps Data Miner by Client Coffee",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/map.png",
      "48": "images/map.png",
      "128": "images/map.png"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.google.com/maps/*",
        "*://maps.google.com/*"
      ],
      "js": [
        "js/maps-data-miner.js"
      ],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "images/map.png",
    "48": "images/map.png",
    "128": "images/map.png"
  }
}